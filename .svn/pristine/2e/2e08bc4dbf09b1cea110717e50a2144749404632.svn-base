@CHARSET "UTF-8";

.highcharts-tracker {
	font-size: 100%;
}

.picker__input ~ .mdl-textfield__label {
	visibility: visible !IMPORTANT;
	top: 18px;
	color: #757575;
}

.picker__input {
	border-bottom-color: #757575
}

.picker__input.picker__input--active {
	border-bottom-color: #4CAF50
}

.picker__input.picker__input--active ~ .mdl-textfield__label {
	color: #4CAF50
}

.col-sm-5.p-0:NTH-CHILD(1) {
	padding-right: 5px !important
}

.col-sm-5.p-0:NTH-CHILD(2) {
	padding-left: 5px !important
}

@media ( max-width : 772px ) {
	.col-sm-5.p-0:NTH-CHILD(1), .col-sm-5.p-0:NTH-CHILD(2) {
		padding-left: 0 !important
	}
}

.img-search {
	width: 100%
}