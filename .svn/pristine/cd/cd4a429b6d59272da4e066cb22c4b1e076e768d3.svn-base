@CHARSET "UTF-8";

/* --------------------------
 *       STUDENT
 * --------------------------
 */
.mdl-card.mdl-student{
    max-width: 250px;
	margin-top: 0px;	
	margin-bottom:10px;
	margin-right: 5px;
	margin-left: 5px;	
	display: inline-block;	
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
	transition: all .3s ease-in-out;
	z-index: 1;
}

.mdl-card.mdl-student .mdl-card__title{
	padding-bottom: 10px;
	padding-top: 60px;
    -webkit-transition: padding-top .3s ease-in-out;
    -moz-transition: padding-top .3s ease-in-out;
    -o-transition: padding-top .3s ease-in-out;
	transition: padding-top .3s ease-in-out;	
}

.mdl-card.mdl-student .mdl-card__title img{
	margin: auto;
	width: 100px;
	height: 100px;
	border-radius: 50%;
}

.mdl-card.mdl-student .mdl-card__supporting-text{
	padding-top: 0px;
	text-align: center;	
}

.mdl-card.mdl-student .mdl-card__supporting-text .icon{
	float: right;
}

.student-desc-state{
	font-size: 14px;	
	margin-bottom: 5px;
}

.student-desc-state i{
	position: relative;
	top: 2.5px;
	font-size: 14px;		
}

.state{
    position: absolute;
    top: 120px;
	right: -webkit-calc(50% - 60px);		
	right: -moz-calc(50% - 60px);		
	right: -ms-calc(50% - 60px);
	right: -o-calc(50% - 60px);	
    right: calc(50% - 60px);	
	text-align: center;
	line-height: 2.25;
    width: 40px;
    height: 40px;
	border: 3px solid #FFFFFF;
    border-radius: 50%;
	background-color: #EEEEEE;	
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
	transition: all .3s ease-in-out;
}

.modal .state{
	position: relative;
	top: inherit;
	right: inherit;
    font-size: 12px;
    padding-right: 5px;
    margin-right: 5px;
    padding: 9px;
	border:none;

}

.state:after{
	content: "-";
}

.state:HOVER{
	cursor: pointer;
}

.datos-incompletos:after,
.pre-registro:after,
.registrado:after,
.promovido:after,
.verano:after,
.retirado:after,
.matriculable:after,
.matriculado:after,
.no-promivido:after,
.nivelacion:after,
.egresado:after{
	font-weight: 500;		
}

/*----- DATOS INCOMPLETOS -----*/
.datos-incompletos,
.label-datos-incompletos{
	background-color: #EEEEEE;	
	color: #757575;
}

.datos-incompletos:after{
	content: "DI";
}

/*----- PRE-REGISTRO -----*/
.pre-registro,
.label-pre-registro{
	background-color: #EEEEEE;	
	color: #757575;
}

.pre-registro:after{
	content: "PR";
}

/*-----   REGISTRO   -----*/
.registrado,
.label-registrado{
	background-color: #E0E0E0;
	color: #9E9E9E;	
}

.registrado:after{
	content: "RE";
}

/*-----   MATRICULABLE   -----*/
.matriculable,
.label-matriculable{
	background-color: #757575;
	color: #FFFFFF;	
}

.matriculable:after{
	content: "MB";
}

/*-----  MATRICULADO  -----*/
.matriculado:after{
	content: "MT";	
}

.student-matriculado ~ .mdl-card__actions .mdl-button{
	color: #3350B9;
}

.matriculado,
.label-matriculado,
.student-matriculado ~ .mdl-card__actions .mdl-button--raised{
	background-color: #3350B9;
	color: #FFF
}

/*-----   PROMOVIDO   -----*/
.promovido:after{
	content: "PV";		
}

.student-promovido ~ .mdl-card__actions .mdl-button{
	color: #8BC34A;
}

.promovido,
.label-promovido,
.student-promovido ~ .mdl-card__actions .mdl-button--raised{
	background-color: #8BC34A;
    color: #FFF;	
}

/*-----    VERANO    -----*/
.verano:after{
	content: "VR";		
}

.student-verano ~ .mdl-card__actions .mdl-button{
	color: #757575;
}

.verano,
.label-verano,
.student-verano ~ .mdl-card__actions .mdl-button--raised{
	background-color: #FFEB3B;
    color: #757575 !important;	
}

/*-----    RETIRADO    -----*/
.retirado:after{
	content: "RT";		
}

.student-retirado ~ .mdl-card__actions .mdl-button{
	color: #757575;
}

.retirado,
.label-retirado,
.student-retirado ~ .mdl-card__actions .mdl-button--raised{
	background-color: #757575;
    color: #FFF;	
}

/*-----    NO PROMOVIDO    -----*/
.no-promovido:after{
	content: "NP";		
}

.student-no-promovido ~ .mdl-card__actions .mdl-button{
	color: #E53935;
}

.no-promovido,
.label-no-promovido,
.student-no-promovido ~ .mdl-card__actions .mdl-button--raised{
	background-color: #E53935;
    color: #FFF;	
}

/*-----    NIVELACION    -----*/
.nivelacion:after{
	content: "NV";
}

.student-nivelacion ~ .mdl-card__actions .mdl-button{
	color: #EF9A9A;
}

.nivelacion,
.label-nivelacion,
.student-nivelacion ~ .mdl-card__actions .mdl-button--raised{
	background-color: #EF9A9A;
    color: #FFF;	
}

/*-----    EGRESADO    -----*/
.egresado:after{	
	content: "EX";	
}

.student-egresado ~ .mdl-card__actions .mdl-button{
	color: #FF9800;
}

.egresado,
.label-egresado,
.student-egresado ~ .mdl-card__actions .mdl-button--raised{
	background-color: #FF9800;
    color: #FFF;	
}

/*----- PUNTUAL -----*/
.pago.puntual .student-name, 
.pago.puntual ~ .mdl-card__actions .mdl-button{
	color: #009688;
}

.pago.puntual ~ .mdl-card__actions .mdl-button--raised{
	background-color: #009688;
	color: #FFF;
}

/*-----   MOROSO   -----*/
.pago.moroso .student-name, 
.pago.moroso ~ .mdl-card__actions .mdl-button{
	color: #F44336;
}

.pago.moroso ~ .mdl-card__actions .mdl-button--raised{
	background-color: #F44336;
	color: #FFF;
}

/* --------------------------
 *    STUDENT ASSISTENCE
 * --------------------------
 */
.mdl-student_assitence{
	width: 154px;
	margin-top: 0px;	
	margin-bottom:10px;
	margin-right: 5px;
	margin-left: 5px;	
	display: inline-block;	
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
	transition: all .3s ease-in-out;
	z-index: 1;
}

.mdl-student_assitence .mdl-card {
	width: 100%;	
	cursor: pointer;
	margin: 0;
	background-color: transparent;
    box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .14), 0 3px 1px -2px rgba(0, 0, 0, .2), 0 1px 5px 0 rgba(0, 0, 0, .12);
	transition: all .3s ease-in-out;
	-webkit-transition: all .3s ease-in-out;
}

.mdl-student_assitence .mdl-card .mdl-card__supporting-text{
	max-height: 165px;
	text-align: center;
}

.mdl-student_assitence .mdl-card .assistance{
	background-color: #EEEEEE;
	height: 25px;
	width: 25px;
	border-radius: 100%;
	position: absolute;
	color: #FFF;
	font-weight: 500;
	text-align: center;
	line-height: 1.75
}

.label-circle{
    height: 20px;
    width: 20px;
	line-height: 20px;
    text-align: center;
    border-radius: 50%;
	font-size: 10px;
    display: inline-block;
    background-color: #EEEEEE;
	color: #FFF;
    vertical-align: middle;
}

.mdl-student_assitence .mdl-card ul{
	text-align: center;
	padding: 0;
	margin: 0;
	vertical-align: middle;
	width: 100%;
	height: 100%;
}

.mdl-student_assitence .mdl-card ul li{
	width: 100%
}

.mdl-student_assitence .mdl-card ul i{
	font-size: 14px;
	margin-right: 5px;
}

.mdl-student_assitence .mdl-card img{
	border-radius: 100%;
	width: 75px;
	height: 75px;
}

.mdl-student_assitence .mdl-card .mdl-card__supporting-text h2{
	margin: 0;
	margin-top: 5px;
	text-align: center;
	font-size: 14px;
	line-height: 1;
	color: #757575
}

.mdl-student_assitence .mdl-card .mdl-card__supporting-text h2:nth-child(2n){
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.bg-present,
.label-circle.present,
.present .assistance{
	background-color: #8BC34A !important
}

.bg-delay,
.label-circle.delay,
.delay .assistance{
	background-color: #FFEB3B !important;
	color: #795548 !important;
}

.bg-delay-justify,
.label-circle.delay-justify,
.delay-justify .assistance{
	background-color: #673AB7 !important
}

.absence{
	opacity: 0.25;
}

.bg-absence,
.label-circle.absence,
.absence .assistance{
	background-color: #757575 !important
}

.bg-absence-justify,
.label-circle.absence-justify,
.absence-justify .assistance{
	background-color: #FF9800 !important;
	color: #FFF !important;		
}

.label-circle.present:after,
.present .assistance:after{
	content: "A";
}

.label-circle.delay:after,
.delay .assistance:after{
	content: "T";	
}

.label-circle.delay-justify:after,
.delay-justify .assistance:after{
	content: "TJ";	
}

.label-circle.absence:after,
.absence .assistance:after{
	content: "F";	
}

.label-circle.absence-justify:after,
.absence-justify .assistance:after{
	content: "FJ";	
}

.i-present{
	color: #8BC34A !important
}

.i-delay{
	color: #FFEB3B !important
}

.i-delay-justify{
	color: #673AB7 !important
}

.i-absence{
	color: #757575 !important
}

/* --------------------------
 *    STUDENT NOTES
 * --------------------------
 */
.mdl-card.mdl-note{	
	width: 154px;
	cursor: pointer; 
	margin-top: 0px;	
	margin-bottom:10px;
	margin-right: 5px;
	margin-left: 5px;	
	display: inline-block;	
    -webkit-transition: all .3s ease-in-out;
    -moz-transition: all .3s ease-in-out;
    -o-transition: all .3s ease-in-out;
	transition: all .3s ease-in-out;
	z-index: 1;
}

.mdl-note .mdl-card__supporting-text{
	text-align: center;
}

.mdl-note .mdl-card__supporting-text img{
	border-radius: 100%;
    width: 70px;
    height: 70px;
}

.mdl-note .mdl-card__supporting-text h2{
	margin: 0;
    margin-top: 16px;
    text-align: center;
    font-size: 14px;
    line-height: 1;
    color: #757575;
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.mdl-note .mdl-card__supporting-text h2:AFTER{
	content: ' ';
    width: 50px;
    height: 14px;
    right: 16px;
    position: absolute;
    z-index: 100;
    text-align: right;
    background-image: -webkit-gradient(linear,left top,right top,color-stop(0%,rgba(255,255,255,0)),color-stop(100%,rgba(255,255,255,1)));
    background-image: -webkit-linear-gradient(left,rgba(255,255,255,0),rgba(255,255,255,1));
    background: linear-gradient(to right,rgba(255,255,255,0),#FFFFFF);	
}

.mdl-note .mdl-button__circle{
    height: 30px;
    width: 30px;
	margin-left: 16px;
    background-color: transparent;
    border-radius: 50%;
    font-size: 10px;
    text-align: center;
    color: #757575;
    line-height: 3;
}

.mdl-note .mdl-value{	
    position: absolute;
    height: 40px;
    width: 40px;
    background-color: transparent;
    top: 47.5px;
    left: 90px;
    text-align: center;
    border-radius: 50%;
    padding: 0;
    line-height: 40px;
}

.mdl-note .mdl-value i{
	font-size: 32px;
	color: #757575
}

.mdl-student_red .mdl-button__circle{
    background-color: #F44336;
	color: #FFFFFF;
}

.mdl-student_green .mdl-button__circle{
    background-color: #4CAF50;	
	color: #FFFFFF;	
}

.mdl-student_ambar .mdl-button__circle{
    background-color: #FFC107;	
	color: rgb(37, 32, 20);
}

.mdl-student_red .mdl-value{
    background-color: #F44336;
	color: #FFFFFF;
}

.mdl-student_green .mdl-value{
    background-color: #4CAF50;	
	color: #FFFFFF;	
}

.mdl-student_ambar .mdl-value{
    background-color: #FFC107;	
	color: rgb(37, 32, 20);
}

.mdl-note .mdl-student__option{
	display: inline-block;
}

.mdl-note .mdl-card__actions{
	text-align: center;
}

.mdl-note .mdl-student__option button{
	margin: 0;
	background-color: #eeeeee;
}

.mdl-note .mdl-student__option button i,
.mdl-student__option label{
	font-size: 16px;
	color: #757575
}

.mdl-note .mdl-student__option label{
	margin: 0
}

.mdl-note .mdl-student__option button.mdl-button--icon ~ label.awards-points {
    position: relative;
    top: 2.5px;
    right: inherit;
    font-size: 12px;
    line-height: 1.75;
}

.awards-points{
	position: absolute;
    top: 25px;
    right: calc(50% - 45px);
    border-radius: 50%;
    height: 24px;
    width: 24px;
    background-color: #F3F3F3;
    border: 2px double #757575;
    color: #757575;
    text-align: center;
    font-size: 12px;
    line-height: 1.75;
	transform: scale(1);
	transition:transform .5s ease;
  	transition-delay:0.25s;	
}

.awards-points.mdl-bounce{
	transform: scale(0);	
}

.awards-points.more-points{	
    background-color: #00BCD4;
    border: 2px double #00BCD4;	
	color: #FFFFFF
}

/* --------------------------
 *       AWARDS 
 * --------------------------
 */
.mdl-awards {
    width: 24.5%;
    display: inline-table;
    border-radius: 0px;
    border:none;
    box-shadow: none;
    margin: 0;
    padding: 16px;
    text-align: center;
}

.mdl-awards{
	opacity: 0;
	transform: scale(0);
	transition: .5s all;
}

.transformed.mdl-awards{
	opacity: 1;
	transform: scale(1);
}

.mdl-awards:hover{
	cursor: pointer;
	
}

.mdl-awards img:hover{ 
    transform: scale(1.05);
    -webkit-transform: scale(1.05);
}

.awards-points{
	position: absolute;
    top: 25px;
    right: calc(50% - 45px);
    border-radius: 50%;
    height: 24px;
    width: 24px;
    background-color: #F3F3F3;
    border: 2px double #757575;
    color: #757575;
    text-align: center;
    font-size: 12px;
    line-height: 1.75;
	transform: scale(1);
	transition:transform .5s ease;
  	transition-delay:0.25s;	
}

.mdl-awards img{
	height: 118px;
	margin: auto;
	margin-bottom: 10px;
	margin-top: 5px;
	transition: all .3s ease-in-out;
}

.mdl-awards label{
    margin: 0;
    width: 148px;
    font-size: 14px;
    height: 35px;
    overflow: hidden;
    text-overflow: ellipsis;
	font-weight: bold;
}

.awards-points.mdl-bounce{
	transform: scale(0);	
}

.awards-points.more-points{	
    background-color: #00BCD4;
    border: 2px double #00BCD4;	
	color: #FFFFFF
}

