/* --------------------------
 *       PHOTO
 * --------------------------
 */
.mdl-photo{
	background-color: #FFFFFF;
    cursor: pointer;
    overflow: hidden;
    width: 100px;
	margin: auto;
	text-align: center;	
	padding: 0;
}

.mdl-img {
	border-radius: 50%;
	width: 100px;
	height: 100px;
	margin: auto 0;
	cursor: pointer;
	border: 3px solid;
	border-color: #FFFFFF;	
	opacity: 1;	
    -webkit-transition: border-color .5s ease-in, opacity .5s ease-in;	
    -moz-transition: border-color .5s ease-in, opacity .5s ease-in;	
    -ms-transition: border-color .5s ease-in, opacity .5s ease-in;
    -o-transition: border-color .5s ease-in, opacity .5s ease-in;	
	transition: border-color .5s ease-in, opacity .5s ease-in;	
}

.mdl-photo:hover .mdl-img{
	opacity: 0.5;	
	border-color: #004062 !important;
}

.mdl-photo .fade-caption i{
	font-size: 42px;
	color: #004062;
	position:absolute;
	right: -webkit-calc(50% - 20.5px);
	right: -moz-calc(50% - 20.5px);	
	right: -ms-calc(50% - 20.5px);	
	right: -o-calc(50% - 20.5px);	
	right: calc(50% - 20.5px);	
    top: 75px;
	opacity: 0;
	-webkit-transform: scale(0);	
	transform: scale(0);
    -webkit-transition: opacity .5s ease-in, transform.25s ease-in;	
	transition: opacity .5s ease-in, transform.25s ease-in;	
}

.mdl-photo:hover .fade-caption i{
	opacity:1;
	-webkit-transform: scale(1);	
	transform: scale(1);
}

