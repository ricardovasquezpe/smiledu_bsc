}*/
.mdl-card__menu {
	top: 16px !important;
	right: 16px;
}

.mdl-card__menu i {
	font-size: 18px
}

.mdl-card {
	opacity: 0.5;
}

.mdl-card.active {
	opacity: 1;
}

.mdl-card__supporting-text a {
	cursor: pointer;
}

.btn.ink-reaction {
	outline: none;
	box-shadow: none
}

.btn.ink-reaction.active, i.active, i.active-o, small.active, small.active-o
	{
	color: #FF9200 !important;
}

.gallery-cell .mdl-card .mdl-card__supporting-text {
	width: 100%;
	min-height: 90px;
}

/* Progress bar */
.progress.progress-striped {
	border-radius: 0px !important
}

/* Posicion del numero de pregunta */
.post-number {
	position: absolute;
	top: 16px;
	left: 16px;
}

/* Style inputs fab */
.fab.active .mdl-textfield.is-upgraded .mdl-textfield__label {
	top: 40px !important
}

.fab.active .mdl-textfield.is-upgraded.is-dirty .mdl-textfield__label,
	.fab.active .mdl-textfield.is-upgraded.is-focused .mdl-textfield__label,
	.fab.active .mdl-textfield.is-upgraded.is-focused.is-dirty .mdl-textfield__label
	{
	top: 20px !important
}

.fab.active .filter-option, .fab.active .mdl-textfield.is-upgraded .mdl-textfield__label
	{
	font-size: 14px !important;
	font-weight: 200 !important;
	color: #757575;
}

.fab.active .mdl-textfield.is-dirty .mdl-textfield__label, .fab.active .mdl-textfield.is-focused .mdl-textfield__label
	{
	font-size: 10px !important;
	font-weight: 400 !important;
}

.fab.active .mdl-textfield.is-focused .mdl-textfield__label {
	color: #FF9800;
}

.bottom {
	bottom: -25%;
}

@media ( min-width : 120px ) {
	#modal-init .cards {
		min-height: 525px;
	}
	.mdl-input-button-group button.mdl-button:before {
		content: "+";
	}
	.mdl-input-button-group .mdl-textfield {
		width: 65%
	}
	.mdl-input-button-group .mdl-button {
		width: 25%;
	}
	.fab.active {
		width: 95%;
		right: calc(2.5%);
		top: inherit;
		top: 5%;
	}
	#modal-init .card, .mdl-card {
		border-radius: 0px;
	}
}

@media ( min-width : 360px ) {
	.mdl-card .mdl-card__supporting-text small.small-very-happy:before {
		content: "Muy sati...";
	}
	.mdl-card .mdl-card__supporting-text small.small-happy:before {
		content: "Satisfecho";
	}
	.mdl-card .mdl-card__supporting-text small.small-normal:before {
		content: "Normal";
	}
	.mdl-card .mdl-card__supporting-text small.small-bad:before {
		content: "Insatisfecho";
	}
	.mdl-card .mdl-card__supporting-text small.small-very-bad:before {
		content: "Muy insa..";
	}
	#modal-init .cards {
		min-height: 500px;
	}
}

@media ( max-width : 480px ) {
	.col-md-4.col-md-offset-4.col-sm-6.col-sm-offset-3.col-xs-8.col-xs-offset-2
		{
		width: 91.33333333%;
		margin-left: 4.33333333%;
	}
	.bottom {
		bottom: -25% !important;
	}
}

@media ( min-width : 500px ) {
	.mdl-card .mdl-card__supporting-text small.small-very-happy:before {
		content: "Muy satisfecho";
	}
	.mdl-card .mdl-card__supporting-text small.small-very-bad:before {
		content: "Muy insatisfecho";
	}
	.mdl-input-button-group button.mdl-button:before {
		content: "AGREGAR";
	}
	.mdl-input-button-group .mdl-textfield {
		width: 70%
	}
	.mdl-input-button-group .mdl-button {
		width: 27.5%;
	}
}

@media ( min-width : 640px ) and (max-height : 480px) {
	.fab.active {
		width: 50%;
		right: calc(25%);
		bottom: 18.75%;
	}
}

@media ( min-width : 640px ) and (min-height : 481px) {
	.fab.active {
		width: 75%;
		right: calc(12.5%);
		top: 5%;
	}
}

@media ( min-width : 780px) and (min-height : 480px) {
	#modal-init .card, .mdl-card {
		border-radius: 5px;
	}
}

@media ( min-width : 920px) and (max-width : 1520px) {
	.mdl-card .mdl-card__supporting-text small.small-very-happy:before {
		content: "Muy satis...";
	}
	.mdl-card .mdl-card__supporting-text small.small-very-bad:before {
		content: "Muy insat...";
	}
}

@media ( min-width : 1200px) {
	.fab.active {
		top: 5%;
		width: 30%;
		right: calc(35%);
	}
	#modal-init .cards {
		min-height: 500px;
	}
}

