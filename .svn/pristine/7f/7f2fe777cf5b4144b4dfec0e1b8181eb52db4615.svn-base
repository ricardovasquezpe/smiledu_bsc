@CHARSET "UTF-8";

header .mdl-layout__tab-bar-container {
	display: none;
}

.modal-dialog small {
	font-size: 85%
}

.col-md-4.col-md-offset-4.col-sm-8.col-sm-offset-2.p-0:LAST-CHILD {
	margin-bottom: 20px
}

.mdl-radio {
	min-width: 150px;
	text-align: left;
}

.mdl-checkbox {
	min-width: 125px;
	margin-bottom: 10px;
	margin-right: 10px;
	text-align: left;
}

small {
	font-size: 70%
}

.fab.active textarea {
	max-height: 150px
}

.gallery-cell .mdl-card .mdl-card__supporting-text .col-xs-2.p-0.m-r-5:LAST-CHILD
	{
	margin-right: 0px !important;
}

/* SELECT STYLE */

/*
.bootstrap-select{
	width: 100% !important;
}*/

/* Contador de preguntas obligatorias */
.question-count {
	position: absolute;
	bottom: 0px;
	right: 35px;
	color: #757575;
}

/*-----link-smiledu----*/
.link-smiledu {
	text-decoration: none;
	list-style: none;
	color: #757575;
}

.link-smiledu strong {
	font-weight: 700;
}

.link-smiledu:hover {
	color: #000;
	list-style: none;
	text-decoration: none;
}

/* Contenido de Card Servicios */
#modal-init .card.card--init, #modal-init .card.card--services {
	max-width: 100%;
}

#modal-init .card.card--servicess .content--services {
	display: flex;
}

/* Items Servicios*/
a.item-select {
	background-color: #FFA900;
	width: auto;
	border-radius: 20px;
	padding: 5px 20px;
	color: #FFF;
	margin: 5px;
	text-decoration: none;
	cursor: pointer;
	float: left;
}

.item-select.active {
	background-color: #00A6D4;
}

.item-select button {
	position: relative;
	top: 3.5px;
}

.item-select i {
	color: #FFF;
}

/* Fap modal */
.fab.active .item-select {
	background-color: #D3d3d3;
}

.fab.active .item-select.active {
	background-color: #FF9200;
}

/* Power by Softhy */
.softhy {
	position: absolute;
	bottom: -50PX;
	text-align: center;
	width: 100%;
	color: #757575;
}

#divPregInicial {
	padding: 20px;
}

/* Panel Inicio */
#modal-init .cards {
	background-color: transparent;
	width: 100%;
	height: 500px
}

#modal-init .card-control {
	display: none;
}

#modal-init .card-control:checked+.card {
	-webkit-clip-path: circle(270% at 50px 88%);
	clip-path: circle(270% at 50px 88%);
	z-index: 500;
	-webkit-transition: -webkit-clip-path 2s ease;
	-moz-transition: -moz-clip-path 2s ease;
	-o-transition: -o-clip-path 2s ease;
	transition: -webkit-clip-path 2s ease;
	transition: clip-path 2s ease;
	transition: clip-path 2s ease, -webkit-clip-path 2s ease;
}

#modal-init .card {
	/*display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;*/
	-webkit-flex-flow: row nowrap;
	-ms-flex-flow: row nowrap;
	flex-flow: row nowrap;
	-webkit-box-pack: justify;
	-webkit-justify-content: space-between;
	-ms-flex-pack: justify;
	justify-content: space-between;
	-webkit-box-align: start;
	-webkit-align-items: flex-start;
	-ms-flex-align: start;
	align-items: flex-start;
	height: 100%;
	position: absolute;
	text-align: center;
	top: 0;
	left: 0;
	-webkit-clip-path: circle(20px at 50px 90%);
	clip-path: circle(20px at 50px 90%);
	-webkit-transition: -webkit-clip-path 0s 1s;
	transition: -webkit-clip-path 0s 1s;
	transition: clip-path 0s 1s;
	transition: clip-path 0s 1s, -webkit-clip-path 0s 1s;
	z-index: 300;
	box-sizing: border-box;
}

#modal-init .card--init {
	background-color: #FFFFFF;
	-webkit-clip-path: circle(0 at 65px 90%);
	clip-path: circle(0 at 65px 90%);
}

#modal-init .img--init {
	max-width: 165px;
	margin-top: 30px;
	margin-bottom: 30px;
	width: 100%;
	height: auto;
}

#modal-init .card--services {
	color: #FFFFFF;
	background-color: #FF9900;
	-webkit-clip-path: circle(0 at 75% 85%);
	clip-path: circle(0 at 75% 85%);
}

#modal-init .main-nav {
	z-index: 99999;
	position: absolute;
	bottom: 30px;
	right: 30px;
	display: inline-flex;
}

.btn-wrapper .send .btn, .btn-wrapper .finish .btn, #modal-init .main-nav .btn
	{
	padding: 6px 24px;
	color: #848484;
}

#modal-init .main-nav .btn:nth-child(1) {
	visibility: hidden;
}

.btn-wrapper .send .btn:nth-child(1), .btn-wrapper .finish .btn:nth-child(2),
	#modal-init .main-nav .btn:nth-child(2) {
	background-color: #FF9900;
	color: #FFFFFF;
}

#modal-init a.btn-service {
	background-color: #FFA900;
	padding: 10px;
	line-height: 50px;
	margin: 5px;
	height: 50px;
	cursor: pointer;
	border-radius: 4px;
	text-decoration: none;
	color: #FFF;
	transition: all 0.5s ease 0s;
	font-size: 14px;
	font-weight: 500;
	text-transform: uppercase;
	letter-spacing: 0;
}

#modal-init a.btn-service.active {
	background-color: #00A6D4;
}

/* Btn cards go */
#listo {
	display: none;
}

#empezar {
	display: block;
}

a.btn-go {
	background-color: transparent;
	position: absolute;
	bottom: 10px;
	right: 20px;
	padding: 6px 24px;
	margin: 10px 1px;
	cursor: pointer;
	border-radius: 5px;
	text-decoration: none;
	color: #FFF;
	font-size: 14px;
	font-weight: 500;
	text-transform: uppercase;
	letter-spacing: 0;
	transition: all 0.5s ease 0s;
}

a.btn-go:HOVER {
	background-color: #FFA900;
}

/* Div Ver mas propuestas mejora*/
#mostrarMasPropM {
	display: none;
	height: 100px;
	overflow-y: scroll;
}

/* BG Modal */
.bg-modal {
	height: 100%;
	width: 100%;
	position: fixed;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	background: #000;
	z-index: 6;
	opacity: 0.5;
	display: none;
}

/* btn - popup */
.dark-overlay {
	position: absolute;
}

.fab {
	display: none;
	z-index: 5;
	cursor: pointer;
	border-radius: 50%;
	position: fixed;
	color: #fff;
	background: #FF9900;
	height: 56px;
	width: 56px;
	right: 20px;
	bottom: 45px;
	box-shadow: 0 5px 5px rgba(126, 111, 111, 0.39);
	-webkit-transition: all 600ms cubic-bezier(0.200, 0.965, 0.000, 1.005);
	-moz-transition: all 600ms cubic-bezier(0.200, 0.965, 0.000, 1.005);
	-o-transition: all 600ms cubic-bezier(0.200, 0.965, 0.000, 1.005);
	transition: all 600ms cubic-bezier(0.200, 0.965, 0.000, 1.005);
}

.fab i {
	color: #FFF;
}

.fab.active i, .fab.active label {
	color: #757575;
}

.fab.active {
	cursor: default;
	background: #FFF;
	border-radius: 5px;
	box-shadow: 0 25px 35px 0 rgba(0, 0, 0, 0.35);
	height: 475px;
	z-index: 8;
}

.fab-icon {
	position: absolute;
	display: block;
	top: 51.5%;
	left: 54.5%;
	-webkit-transform: translate(-12px, -12px);
	-moz-transform: translate(-12px, -12px);
	-o-transform: translate(-12px, -12px);
	-ms-transform: translate(-12px, -12px);
	transform: translate(-12px, -12px);
	line-height: 24px;
	width: 24px;
	-webkit-touch-callout: none;
	-webkit-user-select: none;
	-moz-user-select: none;
	-ms-user-select: none;
	user-select: none;
}

.active .fab-icon {
	display: none;
}

.active .protect {
	background: none;
}

.cntt-wrapper {
	display: none;
}

.active .cntt-wrapper {
	display: block;
}

#fab-hdr {
	position: absolute;
	width: 100%;
	background: #FF9200;
	border-top-left-radius: 5px;
	border-top-right-radius: 5px;
}

#fab-hdr h4 {
	padding-left: 30px;
	font-size: 18px;
	line-height: 32px;
	-moz-osx-font-smoothing: grayscale;
	margin: 24px 0 16px;
	font-weight: 400;
}

#fab-hdr h3 {
	padding-left: 30px;
	font-size: 18px;
}

.cntt {
	padding: 70px 35px 0 35px;
	color: #757575;
}

.btn-wrapper {
	position: absolute;
	right: 0;
	bottom: 0;
	margin-bottom: 8px;
	padding: 6px 32px 6px 16px;
}

/* Selector */
.options .form-group {
	margin: 0;
}

/* Modal Finalizar Opciones */
.send {
	display: block;
}

.finish {
	display: none;
}

/* MagicSuggest */
#modal-init .ms-ctn {
	background-color: #FF9900;
	border: 2px solid #FF9900;
	border-bottom-color: #FFF;
	margin-top: 20px;
	box-shadow: none;
}

#modal-init .ms-ctn input {
	background-color: #FF9900;
	color: #FFF;
}

#modal-init .ms-ctn ::-webkit-input-placeholder {
	color: #FFF;
}

#modal-init .ms-ctn .ms-trigger {
	background-color: #FF9900;
	border: 0px solid #FF9900;
}

#modal-init .ms-ctn .ms-trigger .ms-trigger-ico {
	border-top: 4px solid #fff !important;
}

#modal-init .ms-ctn:ACTIVE, #modal-init .ms-ctn:HOVER, #modal-init .ms-ctn:BEFORE
	{
	border-bottom-color: #FFF;
}

#modal-init .ms-ctn .ms-helper {
	color: #FFF;
	font-size: 12.5px;
	top: -20px;
	right: 2px;
}

#modal-init .ms-ctn .ms-sel-ctn .ms-sel-item {
	background-color: #FFF;
	color: #FF9900;
	border: 5px solid #FFF;
	border-radius: 50px;
}

#encuesta-init .ms-ctn {
	height: 100px;
	border: 2px solid #FFF;
	border-bottom-color: #757575;
	box-shadow: none;
	overflow-y: visible;
}

#encuesta-init .ms-ctn .ms-trigger .ms-trigger-ico {
	border-top: 4px solid #757575;
}

#encuesta-init .ms-ctn:ACTIVE, #encuesta-init .ms-ctn:HOVER,
	#encuesta-init .ms-ctn:BEFORE {
	border-bottom-color: #FF9900;
}

#encuesta-init .ms-ctn:ACTIVE .ms-trigger .ms-trigger-ico,
	#encuesta-init .ms-ctn:HOVER .ms-trigger .ms-trigger-ico,
	#encuesta-init .ms-ctn:BEFORE .ms-trigger .ms-trigger-ico {
	border-top: 4px solid #FF9900;
}

#encuesta-init .ms-ctn .ms-trigger {
	background-color: #FFF;
	border: 0px solid #FFF;
}

#encuseta-init .ms-ctn .ms-sel-ctn {
	overflow-y: hidden;
}

#encuesta-init .ms-ctn .ms-sel-ctn .ms-sel-item {
	background-color: #FF9900;
	color: #FFF;
	border: 5px solid #FF9900;
	border-radius: 50px;
}

/* Toastr */
.toast-success {
	background-color: #333333;
	color: #FFFFFF;
}

/* Select Profile User */
#modal-init .list-icon {
	width: 50px;
	/*height: 50px;
	margin: auto;*/
}

#modal-init .btn-service.selected a.item-select.active {
	background-color: #00A6D4
}

#modal-init .icon, #modal-init .icon-bg {
	-webkit-tap-highlight-color: transparent;
	-webkit-touch-callout: none;
	/* iOS Safari */
	-webkit-user-select: none;
	/* Chrome/Safari/Opera */
	-khtml-user-select: none;
	/* Konqueror */
	-moz-user-select: none;
	/* Firefox */
	-ms-user-select: none;
	/* IE/Edge */
	position: absolute;
	left: 0px;
	user-select: none;
	cursor: pointer;
	transition: .3s;
	width: 100%;
	height: 100%;
	backface-visibility: hidden;
}

#modal-init .icon img {
	width: 50px;
	height: 50px;
	margin: auto;
}

#modal-init .icon-bg {
	background-color: #FF9900;
	height: 50px;
	width: 50px;
	margin: 0 32.5%;
}

#modal-init .icon-bg {
	transform: rotateY(180deg);
	padding: 12.5px;
}

#modal-init .icon-bg .mdi {
	transform: scale(0.2);
	opacity: 0;
	transition: .3s;
	transition-delay: 0s;
	backface-visibility: hidden;
}

#modal-init .selected .icon {
	transform: rotateY(180deg);
}

#modal-init .selected .icon-bg {
	transform: rotateY(0deg);
	/*display: block;*/
}

#modal-init .selected .icon-bg i {
	transform: scale(1);
	opacity: 1;
	transition-delay: .1s;
	font-weight: 500;
}

#modal-init .card--services .form-group.is-empty {
	margin: 10px 0 0 0;
}

.card--services .form-group.is-empty {
	display: none;
}

.card--services .bootstrap-select.btn-group .dropdown-toggle .filter-option,
	.card--services .bootstrap-select.btn-group .dropdown-toggle span.bs-caret span.caret,
	.card--services label.btn.ink-reaction {
	color: #FFFFFF;
}

.card--services label.active {
	border-bottom: 2px solid #FFF;
}

.card--services select.form-control, .card--services select.form-control:focus,
	.card--services button.btn.btn-default, .card--services button.btn.btn-default:hover,
	.card--services button.btn.btn-default:active, .card--services button.btn.btn-default:focus,
	.card--services .btn-default.active, .btn-default:active, .open>.dropdown-toggle.btn-default,
	.card--services .btn-default.active, .btn-default:active, .open>.dropdown-toggle.btn-default:hover
	{
	border-bottom: 1px solid #FFF;
	color: #FFF;
}

.card--services .bs-searchbox input.form-control, .card--services .bs-searchbox input.form-control:hover,
	.card--services .bs-searchbox input.form-control:active,
	.card--services .bs-searchbox input.form-control:focus {
	border-bottom: 1px solid #9e9e9e;
	color: #9e9e9e;
}

.dropdown-menu>.active>a, .dropdown-menu>.active>a:focus, .dropdown-menu>.active>a:hover
	{
	background-color: #FF9200
}

.card--services .btn-group.bootstrap-select.form-control.pickerButn button span.bs-caret .caret
	{
	border-top-color: #FFF
}

.modal-backdrop.fade {
	opacity: 0.9;
	z-index: -1;
}

.highcharts-data-labels .highcharts-tracker>tspan {
	font-size: 19px
} /*

.info::-webkit-scrollbar {
	width: 4px;
	height: 5px;
}

.info::-webkit-scrollbar-track {
	background: rgba(0, 0, 0, 0.1);
}

.info::-webkit-scrollbar-thumb {
