@CHARSET "ISO-8859-1";

/* Etiquetas */
html, body, .mdl-layout {
	background-color: #e5e6e6;
	overflow-y: hidden !important;
}

header{
	background-color: #FFF !important;
	align-items: baseline;
}

header.mdl-layout__header{
	z-index: 6;
	-webkit-transform: top .5s ease;
	-moz-transition: top .5s ease;
	-ms-transition: top .5s ease;
	-o-transition: top .5s ease;
	transition: top .5s ease;
}

header i {
	color: #757575 !important;
}

main.mdl-layout__content{
	background-color: #e5e6e6;
	padding: 0 !important;
	position: fixed;
	bottom: 0;
	right: 0;
	left: 0;
	top: 100%;
	opacity: 0;
	visibility: hidden;
	-webkit-transform: top .5s ease, opacity 1s linear, visibility .5s ease;	
	-moz-transition: top .5s ease, opacity 1s linear, visibility .5s ease;	
	-ms-transition: top .5s ease, opacity 1s linear, visibility .5s ease;
	-o-transition: top .5s ease, opacity 1s linear, visibility .5s ease;	
	transition: top .5s ease, opacity 1s linear, visibility .5s ease;
	z-index: 5;	
}

main.mdl-layout__content.is-visible{
	position: relative;
	top: 0px;
	visibility: visible;
	opacity: 5
}

main section{
	padding: 15px; 
}

section.mdl-layout__tab-panel{
	-webkit-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -webkit-transform .125s ease-out;
	-webkit-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -moz-transform .125s ease-out;
	-webkit-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-webkit-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-webkit-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -o-transform .125s ease-out;
	-webkit-transition: opacity 1s linear, visibility .5s ease, top 1s ease, transform .125s ease-out;
	-moz-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -webkit-transform .125s ease-out;
	-moz-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -moz-transform .125s ease-out;
	-moz-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-moz-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-moz-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -o-transform .125s ease-out;
	-moz-transition: opacity 1s linear, visibility .5s ease, top 1s ease, transform .125s ease-out;
	-ms-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -webkit-transform .125s ease-out;
	-ms-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -moz-transform .125s ease-out;
	-ms-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-ms-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-ms-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -o-transform .125s ease-out;
	-ms-transition: opacity 1s linear, visibility .5s ease, top 1s ease, transform .125s ease-out;
	-o-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -webkit-transform .125s ease-out;
	-o-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -moz-transform .125s ease-out;
	-o-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-o-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	-o-transition: opacity 1s linear, visibility .5s ease, top 1s ease, -o-transform .125s ease-out;
	-o-transition: opacity 1s linear, visibility .5s ease, top 1s ease, transform .125s ease-out;
	transition: opacity 1s linear, visibility .5s ease, top 1s ease, -webkit-transform .125s ease-out;
	transition: opacity 1s linear, visibility .5s ease, top 1s ease, -moz-transform .125s ease-out;
	transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	transition: opacity 1s linear, visibility .5s ease, top 1s ease, -ms-transform .125s ease-out;
	transition: opacity 1s linear, visibility .5s ease, top 1s ease, -o-transform .125s ease-out;
	transition: opacity 1s linear, visibility .5s ease, top 1s ease, transform .125s ease-out;
}

@media ( max-width : 320px ){
	main section{
		padding: 15px 5px;		
	}	
}

/*PRE LOADER*/
.screen-load {
	background-color: transparent;	
	position: fixed;
	bottom: 0;
	right: 0;
	left: 0;
	text-align: center;
	top: 100%;
	opacity: 1;
	visibility: visibility;	
	-webkit-transform: top 0s ease 1s, opacity 1s linear, visibility 2s ease 1.5s;	
	-moz-transition: top 0s ease 1s, opacity 1s linear, visibility 2s ease 1.5s;	
	-ms-transition: top 0s ease 1s, opacity 1s linear, visibility 2s ease 1.5s;
	-o-transition: top 0s ease 1s, opacity 1s linear, visibility 2s ease 1.5s;	
	transition: top 0s ease 1s, opacity 1s linear, visibility 2s ease 1.5s;
	z-index: 2;
}

.screen-load .mdl-spinner{
	height: 48px;
	width: 48px
}

.screen-load .mdl-spinner .mdl-spinner__circle{
	border-width: 4px
}

.screen-load.screen-load__tab{
	-webkit-transform: top 0s ease, opacity 1s linear, visibility .5s ease 1s;	
	-moz-transition: top 0s ease, opacity 1s linear, visibility .5s ease 1s;	
	-ms-transition: top 0s ease, opacity 1s linear, visibility .5s ease 1s;
	-o-transition: top 0s ease, opacity 1s linear, visibility .5s ease 1s;	
	transition: top 0s ease, opacity 1s linear, visibility .5s ease 1s;	
}


a,
a:FOCUS,
a:ACTIVE,
a:HOVER,
a:VISITED{
	text-decoration: none !important
}

label {
	font-weight: 100;
}

i {
	color: #757575;
}

ul, ol {
	list-style: none;
	margin: 0;
}

/* Body */
body.bg-fab-black .opacidad-fab{
	background-color: rgba(0, 0, 0, .5);
    position: absolute;
    top: 0;
    left: 0;
    height: 100%;
    width: 100%;
    z-index: 10;
    visibility: visible;
    transition-property: background-color;
    transition-duration: .2s;
    transition-timing-function: cubic-bezier(.4, 0, .2, 1);	
}

.mdl-layout__container{
	background-color: #e5e6e6
}

/* HEADER */
header .mdl-layout__header-row{
	padding: 0 16px 0 60px;
}

header.mdl-layout__header .mdl-navigation a ,
header.mdl-layout__header .mdl-navigation button{
	margin: 0 0 0 15px;
}

header.mdl-layout__header .mdl-layout-title{
	line-height: 3;
}

header.mdl-layout__header .mdl-layout-title a {
	text-decoration: none;
	color: #FFF;
	font-size: 18px;
	font-weight: normal;
}

header .mdl-section-head{
	position: absolute;
	font-size: 18px;
	width: 315px;
	left: 165.67px;
    top: 21.2px;
	border-left: 1px solid #004062;	
	padding-left: 20px;
	color: #004062;	
	text-overflow: ellipsis;
	overflow: hidden; 
	white-space: nowrap;	
}

@media ( max-width : 768px ) {	
	header.mdl-layout__header .mdl-layout-title a,
	header.mdl-layout__header .mdl-layout-title a img{
		display: none
	}

	header .mdl-section-head{
		left: 0px;
		top: -10.5px;			
		padding: 0;
		border: none
	}
}

/* MENU BUTTON */
header .mdl-layout__drawer-button{
	outline: none;
}

header .mdl-layout__drawer-button i{
	font-family: Material Icons;
}

header .mdl-layout__drawer-button i:BEFORE{
    content: "\eb33";
	color: #757575;
}

/* Scrool bar */
::-webkit-scrollbar {
	width: 8px;
	height: 5px;
}

::-webkit-scrollbar-track {
	background: rgba(0, 0, 0, 0.1);
}

::-webkit-scrollbar-thumb {
	background: rgba(0, 0, 0, 0.2);
}

.table-responsive{
	margin-bottom: 0
}

@media ( max-device-width: 640px ) {
	main::-webkit-scrollbar {
		width: 0px;
		height: 0px;
	}
	
	main::-webkit-scrollbar-track,
	main::-webkit-scrollbar-thumb {
		background: transparent;
	}
}

/* Button Return */
header a.mdl-button__return {
	position: absolute;
	top: 7px;
	left: 7px;
	visibility: hidden;
	cursor: pointer;
	text-decoration: none;
	height: 48px;
	width: 48px;
	padding: 10px 12px;
	z-index: 0;
}

header.mdl-layout__header.mdl-layout__header__return a.mdl-button__return{
	z-index: 5;
	visibility: visible;
}

header.mdl-layout__header.mdl-layout__header__return .mdl-layout__drawer-button{
	z-index: 0 !important;
	visibility: hidden;
}

header a.mdl-button__return:HOVER,
header .mdl-layout__drawer-button:HOVER {
	background: rgba(0, 0, 0, .2) !important;
	border-radius: 50% !important;
}

/* Header Search */
.mdl-header-input-group {
	position: absolute;
    left: 425px;
    top: 10px;
    right: 375px;
	padding: 0;
	margin: 0;
	height: 44px;
	background-color: #F5F5F5;
	border-radius: 25px;
	display: block;
	z-index: 6;
}

.mdl-icon.mdl-right,
#openSearch,
#buttonSearch{
