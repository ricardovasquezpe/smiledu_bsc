/* Cards */
.mdl-card {
	width: 100%;
	margin-bottom: 10px;
	box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .14), 0 3px 1px -2px
		rgba(0, 0, 0, .2), 0 1px 5px 0 rgba(0, 0, 0, .12)
}

.mdl-card:HOVER{
	z-index: 2	
}

.mdl-card .mdl-card__title{
	min-height: 30px	
}

@media (max-width: 420px){
	.mdl-card__title h2.mdl-card__title-text{
		max-width: 240px;
	}
}

.mdl-card__title-text {
    color: #757575;
	font-size: 18px;
	font-weight: 500
}

.mdl-card .mdl-card__actions{
	text-align: right;
	padding: 0 16px 16px;
}

/* Modales */
.modal{
	z-index: 32
}

.modal-dialog {
	z-index: 566
}

div.modal-backdrop.fade{
	z-index: 31;
}

.modal-backdrop.fade.in{
	height: 5000px !important;
}

/* Modal Card - Fab */
.modal-content{
	border: none;
	border-radius: 5px;
	background-color: transparent;	
}

.mdl-card-fab{
	background-color: transparent;	
}

.mdl-card-fab .mdl-card__title {
	padding-top: 20px;
	padding-bottom: 20px;
	background-color: #004062;
}

.modal .mdl-card__title h2.mdl-card__title-text{
	color: #757575;
}

.mdl-card-fab .mdl-card__title h2.mdl-card__title-text{
	color: #FFFFFF !important;
}

/*.modal .mdl-card__actions,
.mdl-card-fab .mdl-card__actions{
	text-align: right;
	padding: 0 0 16px 16px;
}*/

.modal .mdl-card__actions button.mdl-button,
.mdl-card-fab .mdl-card__actions button.mdl-button{
	color: #004062;
}

.modal .mdl-card__actions a.mdl-button--raised,
.mdl-card-fab .mdl-card__actions a.mdl-button--raised,
.modal .mdl-card__actions button.mdl-button--raised,
.mdl-card-fab .mdl-card__actions button.mdl-button--raised{
