}

/* Botones */
button.mdl-button, 
a.mdl-button {
	text-decoration: none;
	color: #004062;
	font-weight: 500;
	margin: 0px;
	margin-left: 5px	
}

.btn-default.active.focus,
.btn-default:active:hover, 
.open>.dropdown-toggle.btn-default:focus, 
.open>.dropdown-toggle.btn-default:hover{
	color: #757575;
}

.form-group .form-control ~ label:after,
.form-group .form-control ~ .form-control-line:after{
	background-color: #004062 !important;
}

/* Form control all <e> */
.form-control:focus{
	box-shadow: none;
	border-color: transparent;	
}

/* Arbol de modulos */
#navBar .mdl-layout-title a {
    color: #757575;
    font-size: 14px;
    line-height: 0;
    font-weight: 600;
}

.mdl-layout .mdl-layout__drawer .mdl-layout-title img, .mdl-layout .mdl-layout__header-row .mdl-layout-title img
	{
	height: 37.5px;
	margin-right: 10px;
}

.mdl-layout__drawer .mdl-navigation {
	padding: 0 !important;
}

.mdl-layout__drawer .mdl-navigation .mdl-navigation__link {
	padding: 10px 20px;
}	

.mdl-layout__drawer>.mdl-layout-title {
	font-size: 18px;
	border-bottom: 1px solid rgba(158, 158, 158, .2);
}

.mdl-nav {
	width: 100%;
	margin: 0;
	padding: 0;
}

.mdl-nav li.separator {
	border-top: 1px solid rgba(158, 158, 158, .2);
	padding: 0;
}

.mdl-nav ul {
	*zoom: 1;
	list-style: none;
	margin: 0;
	padding: 0;
}

.mdl-nav ul:before, .mdl-nav ul:after {
	content: "";
	display: table;
}

.mdl-nav ul:after {
	clear: both;
}

.mdl-nav ul>li {
	position: relative;
}

.mdl-nav a {
	display: block;
	padding: 15px 20px 15px 60px;
	color: #757575 !important;
	text-decoration: none;
}

.mdl-layout__drawer .mdl-nav li ul li a,
.mdl-layout__drawer--right .mdl-nav li ul li a{
	font-weight: 400 !important;
}

.mdl-layout__drawer .mdl-nav a,	
.mdl-layout__drawer--right .mdl-nav a{
	font-weight: 600;
}

.mdl-nav a i {
	position: absolute;
	right: 20px;
	left: 20px
}

.mdl-nav a i:FIRST-CHILD {
	top: 7px;
}

.mdl-nav a.dont-three i:FIRST-CHILD {
	position: absolute;
	left: 20px;
	top: 13.5px;
}

.mdl-nav li ul {
	width: 100%;
	padding: 0;
}

.mdl-nav li ul li {
	width: 100%;
}

.mdl-nav li ul li a {
	padding-left: 80px;
}

.mdl-nav a.mdl-button{
	border-radius: 0 !important;
	height: auto;
	margin: 0 !important;
	text-transform: initial;
	text-align: left;
	line-height: normal;
}

.mdl-nav li ul li a i {
	position: absolute;
	left: 40px !important;
}

.mdl-nav li ul {
	position: relative;
	left: 0;
	z-index: 1;
}

.mdl-nav li ul li {
	overflow: hidden;
	height: 0;
	-webkit-transition: height 200ms ease-in;
	-moz-transition: height 200ms ease-in;
	-o-transition: height 200ms ease-in;
	transition: height 200ms ease-in;
}

.mdl-nav ul>li:hover ul li {
	height: 60px;
}

@media ( max-width : 640px) {
	.mdl-menu__container {
		margin-right: 25px;
	}
}

/* ADD MENU COLOR */
.mdl-nav li.active {
	background-color: rgba(158, 158, 158, .2);
}

.mdl-nav li.active a,
.mdl-nav li.active a i{
	color: #004062 !important;
}

/* Navs right */
.mdl-layout__drawer--right {
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-flex-direction: column;
	-ms-flex-direction: column;
	flex-direction: column;
	-webkit-flex-wrap: nowrap;
	-ms-flex-wrap: nowrap;
	flex-wrap: nowrap;
	width: 300px;
	height: 100%;
	max-height: 100%;
	position: absolute;
	top: 0;
	right: 0;
	box-shadow: 0 2px 2px 0 rgba(0, 0, 0, .14), 0 3px 1px -2px
		rgba(0, 0, 0, .2), 0 1px 5px 0 rgba(0, 0, 0, .12);
	box-sizing: border-box;
	border-right: 1px solid #e0e0e0;
	background: #fafafa;
	-webkit-transform: translateX(310px);
	transform: translateX(310px);
	-webkit-transform-style: preserve-3d;
	transform-style: preserve-3d;
	will-change: transform;
	transition-duration: .2s;
	transition-timing-function: cubic-bezier(.4, 0, .2, 1);
	transition-property: transform;
	transition-property: transform, -webkit-transform;
	color: #424242;
	overflow: visible;
	overflow-y: auto;
	z-index: 32
}

.mdl-layout__drawer--right.is-visible {
	-webkit-transform: translateX(0px);
	transform: translateX(0px)
}

.mdl-layout__drawer--right.is-visible ~.mdl-layout__content.mdl-layout__content{
	overflow: hidden
}

.mdl-layout__drawer--right>* {
	-webkit-flex-shrink: 0;
	-ms-flex-negative: 0;
	flex-shrink: 0
}

.mdl-layout__drawer--right >.mdl-layout__title,
	.mdl-layout__drawer--right >.mdl-layout-title {
	line-height: 64px;
	padding-left: 40px;
	border-bottom: 1px solid rgba(158, 158, 158, .2);
	background: url(../img/menu/avantgard_profile.jpg) no-repeat;
	background-size: 100%;
}

