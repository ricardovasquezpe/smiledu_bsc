.mdl-app_content .mdl-card__front .mdl-app_text label{	
	overflow: hidden;
	white-space: nowrap;
	font-size: 13px;
	line-height: 20px;
	width: -webkit-calc(100% - 20px);
	width: -moz-calc(100% - 20px);
	width: -ms-calc(100% - 20px);
	width: -o-calc(100% - 20px);
	width: calc(100% - 20px);
}

.mdl-app_content .mdl-card__front .mdl-app_text i{
	position: absolute;
	right: 5.5px;
	top: 5.5px;
	font-size: 18px;
}

.mdl-app_content .mdl-card__back{
	background-color: #FAFAFB;
	-webkit-transform: rotateY(-180deg);
	-moz-transform: rotateY(-180deg);
	-o-transform: rotateY(-180deg);
	transform: rotateY(-180deg);	
}

.mdl-app_content .mdl-card__back h4{
	width: 100%;
	padding: 0;
	margin: 0;
	padding: 10px;	
	font-size: 13px;
	text-align: left;
}

.mdl-app_content .mdl-card__back ul{
	margin: 0;
	padding: 0;
	text-align: left;
	overflow: auto;
	width: 100%;
	max-height: -webkit-calc(100% - 41px);
	max-height: -moz-calc(100% - 41px);
	max-height: -ms-calc(100% - 41px);
	max-height: -o-calc(100% - 41px);
	max-height: calc(100% - 41px);	
}

.mdl-app_content .mdl-card__back ul li{
	width: 100%;
	max-width: 100%;
	padding: 5px 10px;
	line-height: 2.5;
}

.mdl-app_content .mdl-card__back ul li:HOVER{
	background-color: rgba(158, 158, 158, .2);
}

.mdl-app_content .mdl-card__back ul li label{
	font-size: 13px;
	line-height: 13px;
	position: relative;
	top: 5px;
	cursor: pointer;
	overflow: hidden;
	white-space: nowrap;
	width: -webkit-calc(100% - 21px);
	width: -moz-calc(100% - 21px);
	width: -ms-calc(100% - 21px);
	width: -o-calc(100% - 21px);
	width: calc(100% - 21px);
	color: #757575;
	font-weight: 500
}

.mdl-app_content .mdl-card__back ul li i{
	position: relative;
	left: 0;
	margin-right: 2.5px;
	font-size: 18px;
	font-weight: lighter;
	cursor: pointer;
}

.mdl-app_content .mdl-card__back h4:AFTER,
.mdl-app_content .mdl-card__back ul li label:AFTER,
.mdl-app_content .mdl-card__front .mdl-app_text label:AFTER {
	content: ' ';
    width: 35px;
    height: 20px;
    right: 25px;
    top: 7.5px;
    position: absolute;
    z-index: 3;
    text-align: right;
    background-image: -webkit-gradient(linear,left top,right top,color-stop(0%,rgba(255,255,255,0)),color-stop(100%,rgba(250, 250, 251, 1)));
    background-image: -webkit-linear-gradient(left,rgba(255,255,255,0),rgba(250, 250, 251, 1));
    background: linear-gradient(to right,rgba(255,255,255,0),#FAFAFB);
}

.mdl-app_content .mdl-card__back ul li:HOVER label:AFTER{
    background-image: -webkit-gradient(linear,left top,right top,color-stop(0%,rgba(255,255,255,0)),color-stop(100%,rgb(232, 232, 233)));
    background-image: -webkit-linear-gradient(left,rgba(255,255,255,0),rgb(232, 232, 233));
    background: linear-gradient(to right,rgba(255,255,255,0),rgb(232, 232, 233));
}

.mdl-app_content .mdl-card__back h4:AFTER {
	top: 7.5px;
    right: 0;
}

.mdl-app_content .mdl-card__back ul li label:AFTER {
    top: -2.5px;
    right: 0;
    height: 15px;
}

.mdl-app_content:HOVER .mdl-card__front,
.mdl-app_content:HOVER .mdl-card__back {
	box-shadow: 0 2px 15px rgba(0,0,0,0.25), 0 10px 10px rgba(0,0,0,0.22);
	z-index: 2;	
}

.mdl-app_content.mdl-flipped .mdl-card__front {
	z-index: 0;
}

.mdl-app_content.mdl-flipped .mdl-card__back {
	z-index: 1;
}

.mdl-app_content.mdl-flipped .mdl-card__front {
	-webkit-transform: rotateY(-180deg);
	-moz-transform: rotateY(-180deg);
	-o-transform: rotateY(-180deg);
	transform: rotateY(-180deg);
}

.mdl-app_content.mdl-flipped .mdl-card__back {
	-webkit-transform: rotateY(0deg);
	-moz-transform: rotateY(0deg);
	-o-transform: rotateY(0deg);
	transform: rotateY(0deg);
}

.ui-sortable-helper .mdl-card__front,
.ui-sortable-helper .mdl-card__back,
.ui-sortable-helper.mdl-app_content:HOVER .mdl-card__front,
.ui-sortable-helper.mdl-app_content:HOVER .mdl-card__back{
	box-shadow: none !important;
	z-index: 3;	
	-webkit-transform: scale(.875);
	-moz-transform: scale(.875);
	-o-transform: scale(.875);
	transform: scale(.875);	
}

.ui-sortable-helper .mdl-card__back,
.ui-sortable-helper.mdl-app_content:HOVER .mdl-card__back{
	z-index: 2
}

