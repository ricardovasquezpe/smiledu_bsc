/* Fab only */
.mfb-only-btn{
    padding: 0;
    margin: 0;
}

.mfb-only-btn button.mfb-component__button--main,
.mfb-only-btn button.mfb-component__button--child{
	position: inherit;
}

.mfb-only-btn .mfb-component__button--main i{
	transform: rotate(0);
	position: inherit;	
}

/* Fab save */
.mfb-only__save{
	height: 64px;
	width: 64px;
	overflow: hidden;
	cursor: pointer;
}

.mfb-only__save .mfb-component__button--main{
	position: absolute;
	top: 4px;
	left: 4px;
}

.mfb-only__save .mfb-component__button--main[disabled="disabled"]{
	cursor: wait;
}

.mfb-only__save .mfb-component__main-icon--resting,
.mfb-only__save .mfb-component__main-icon--active{
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-align: center;
	-webkit-align-items: center;
	    -ms-flex-align: center;
	        align-items: center;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	    -ms-flex-pack: center;
	        justify-content: center;
	-webkit-transition: opacity 0s linear;
	transition: opacity 0s linear;
	color: #FFF;
}

.mfb-only__save .mfb-component__main-icon--active{
	opacity: 0
}

.mfb-only__save.mfb-save__load .mfb-component__main-icon--active{
	opacity: 1;
	-webkit-transition-delay: 3s;
			transition-delay: 3s;
}

.mfb-only__save .mfb-component__main-icon--resting{
	opacity: 1
}

.mfb-only__save.mfb-save__load .mfb-component__main-icon--resting{
	opacity: 0;
	-webkit-transition-delay: 3s;
			transition-delay: 3s;
}

.mfb-load,
.mfb-load:BEFORE{
	position: absolute;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;	
}

.mfb-load{
	animation: loader 2.75s linear;	
}

.mfb-load:BEFORE {
	content: '';
	border-radius: 64px;	
	border-top:10px solid transparent;
    border-bottom:10px solid transparent;
    border-left:10px solid transparent;
    border-right:10px solid transparent;
}

@keyframes loader {
	0% {
		transform: rotate(0deg);
		border-radius: 64px;	
		border-top:10px solid #795548;
	    border-bottom:10px solid transparent;
	    border-left:10px solid transparent;
	    border-right:10px solid transparent;	
	}
	50% {
		transform: rotate(180deg);
		border-radius: 64px;	
		border-top:10px solid #795548;
	    border-bottom:10px solid transparent;
	    border-left:10px solid transparent;
	    border-right:10px solid #795548;
	}
	75% {
		transform: rotate(270deg);
		border-radius: 64px;	
		border-top:10px solid #795548;
	    border-bottom:10px solid #795548;
	    border-left:10px solid transparent;
	    border-right:10px solid #795548;
	}
	100% {
		transform: rotate(360deg);
		border-radius: 64px;	
		border-top:10px solid #795548;
	    border-bottom:10px solid #795548;
	    border-left:10px solid #795548;
	    border-right:10px solid #795548;
	}
}

