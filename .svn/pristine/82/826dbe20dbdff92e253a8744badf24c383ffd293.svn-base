/* Fab position */
.mfb-component--br{
	right: 49.5px;
	bottom: 32.5px;
	-webkit-transition: all 2000ms cubic-bezier(1, 0.965, 0.000, 1.005);
	transition: all 2000ms cubic-bezier(1, 0.965, 0.000, 1.005);	
}

.mfb-component--br .mfb-component__wrap{
	padding: 0;
	margin: 0;
	border-radius: 50%
}

.mfb-component--br .mfb-component__button--main{
	background-color: #004062;
	padding: 8px 0;	
	transform: scale(0);
}

.mfb-component__button--main.is-up{
	transform: scale(1);
	transition:transform .5s ease;
  	transition-delay:0.25s;	
}

.mfb-component__main-icon--active, 
.mfb-component__main-icon--resting{
    -webkit-transform: scale(1) rotate(-180deg);
    transform: scale(1) rotate(-180deg);
	-webkit-transition-duration: .5s; 
	-moz-transition-duration: .5s;	
	-o-transition-duration: .5s;	
	transition-duration: .5s;	
}

.mfb-component--br a.mfb-component__button--main i,
.mfb-component--br button.mfb-component__button--main i{
    position: absolute;
	font-size: 32px;	
    top: 7.5px;
    left: 0;
	color: #FFFFFF !important;	
}

.mfb-component--br a.mfb-component__button--child,
.mfb-component--br button.mfb-component__button--child{
	padding: 5px;
}

.mfb-component--br a.mfb-component__button--child i,
.mfb-component--br button.mfb-component__button--child i{
    top: calc(52% - 15px);
    left: calc(49% - 19px);	
	color: #004062 !important;	
}

.mfb-component--br .mfb-component__button--main{
	box-shadow: 0 6px 10px 0 rgba(0,0,0,0.3)
}

.mfb-component--br .mfb-component__button--child{
	box-shadow: 0 6px 10px 0 rgba(0,0,0,0.14),0 1px 18px 0 rgba(0,0,0,0.12),0 3px 5px -1px rgba(0,0,0,0.2)	
}

.mfb-component--br .mfb-component__button--main:NTH-CHILD(1){
	position: absolute;
	z-index: 1
}

.mfb-component--br .mfb-component__button--main:NTH-CHILD(2){
	z-index: 0;
	box-shadow: none
}

.mfb-component--br:HOVER .mfb-component__button--main[data-mfb-label]:NTH-CHILD(2):after{
	display: none
}

.mfb-component--br:HOVER .mfb-component__button--main.active[data-mfb-label]:NTH-CHILD(2):after{
	display: block
}

.mfb-component--br .mfb-component__button--main.active:NTH-CHILD(2){
	z-index: 2
}

@media ( max-width : 750px ) {
	.mfb-component--br{
		right: 32.5px;
		bottom: 15px;
	}
}

.mfb-component--br .mfb-only-btn .mfb-component__button--main:NTH-CHILD(1){
	position: inherit;
}

.mfb-component--br .mfb-only-btn .mfb-component__button--main i{
	transform: rotate(0)
}

