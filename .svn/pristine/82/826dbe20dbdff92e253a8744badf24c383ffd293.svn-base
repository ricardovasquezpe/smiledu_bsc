/*  BUTTON LOADING */
.mdl-save__load {
	max-width: 100px;
	max-height: 100px;
	transition: .3s all;
}

.mdl-save__load .mdl-spinner.is-active .mdl-spinner__layer-4,
.mdl-save__load .mdl-spinner.is-active .mdl-spinner__layer-3,
.mdl-save__load .mdl-spinner.is-active .mdl-spinner__layer-2,
.mdl-save__load .mdl-spinner.is-active .mdl-spinner__layer-1{
	border-color:#FFFFFF !important;
}

.mdl-save__load .mdl-spinner {
	display: none;
    width: 23px;
    height: 23px;
	transition: .3s all;	
}

.mdl-save__load.mdl-save__load__loading {
	border-radius: 50%;
	min-width: 40px;	
	max-width: 40px;	
	min-height: 40px;
	max-height: 40px;	
	padding: 0;
	padding-top: 7.5px;
	cursor: no-drop;
	transition: all .3s ease-in-out;
}

a.mdl-button.mdl-save__load.mdl-save__load__loading[disabled],
button.mdl-button.mdl-save__load.mdl-save__load__loading[disabled]{
	background-color: #004062  !important;
}

.mdl-save__load.mdl-save__load__loading .mdl-spinner{
	display: inline-block;
}

.mdl-save__load:AFTER {
	content: "GUARDAR";
	font-weight: 300;
}

.mdl-save__load.accept:AFTER{
	content: "ACEPTAR";
}

.mdl-save__load.download:AFTER{
	content: "DESCARGAR";
}
.mdl-save__load.delete:AFTER{
	content: "ELIMINAR";
}

.mdl-save__load.send:AFTER{
	content: "ENVIAR";
}

.mdl-save__load.create:AFTER{
	content: "CREAR";
}

.mdl-save__load.in:AFTER{
	content: "INGRESAR";
}

.mdl-save__load.out:AFTER{
	content: "ANULAR";
}

.mdl-save__load.asign:AFTER{
	content: "ASIGNAR";
}

.mdl-save__load.invit:AFTER{
	content: "INVITAR";
}

.mdl-save__load.download{
	max-width: 115px;
}

.mdl-save__load.mdl-save__load__loading:AFTER {
	content: " ";
}

.mdl-save__load.mdl-save__load__loading::after {
	animation: fadeIn .5s ease-in , spin 1.3s infinite ease-in-out;
	animation-fill-mode: forwards;
	transition: all .5s ease-in-out;
}

@keyframes spin {
	0%, 5% {
		transform: rotate(0deg);
	}
	95%, 100% {
		transform: rotate(360deg);
	}
}

@keyframes fadeIn {
	0% {
		opacity: 0;
	}
	100% {
		opacity: 1;
	}
}

